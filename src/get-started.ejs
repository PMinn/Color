<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Color.css</title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css">
  <link rel="stylesheet" href="./src/japanese-traditional.css">
  <style>
    body {
      background-color: var(--KACHI);
      color: rgb(139, 148, 158);
    }

    .navbar {
      background-color: var(--KUROTSURUBAMI);
      color: #fff;
    }

    .navbar-brand {
      width: 100px;
    }

    .title {
      color: rgb(201, 209, 217);
      /* text-align: center; */
    }
  </style>
</head>

<body>
  <%- include('./partials/nav.ejs') %>
  <div class="container-fluid mt-5 px-3 pt-3">
    <h2 class="title">Get started</h2>
    <p>
      chose a type of color
    <ul>
      <% nav.forEach(color=> { %>
      <li><a href="./<%= color.id %>.html"><%= color.name %> Colors</a></li>
      <% }) %>
    </ul>
    </p>
  </div>
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"></script>
</body>

</html>